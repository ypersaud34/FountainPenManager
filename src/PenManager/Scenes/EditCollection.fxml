<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PenManager.EditingController">
   <children>
      <Button layoutX="403.0" layoutY="451.0" mnemonicParsing="false" onAction="#backToModifyCollectionMenu" text="Back" />
      <Label layoutX="329.0" layoutY="14.0" prefHeight="29.0" prefWidth="193.0" text="Select a Pen to Edit">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TableView fx:id="table" layoutX="25.0" layoutY="56.0" prefHeight="366.0" prefWidth="800.0">
         <columns>
            <TableColumn fx:id="penID" minWidth="100.0" prefWidth="100.0" text="Number" />
            <TableColumn fx:id="modelName" minWidth="100.0" prefWidth="100.0" text="Name" />
            <TableColumn fx:id="brand" minWidth="100.0" prefWidth="100.0" text="Brand" />
            <TableColumn fx:id="color" minWidth="100.0" prefWidth="100.0" text="Color" />
            <TableColumn fx:id="price" minWidth="100.0" prefWidth="100.0" text="Price" />
            <TableColumn fx:id="nib" minWidth="100.0" prefWidth="100.0" text="Nib" />
            <TableColumn fx:id="fillingMechanism" minWidth="100.0" prefWidth="100.0" text="Mechanism" />
            <TableColumn fx:id="dateEntered" minWidth="100.0" prefWidth="100.0" text="Date" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
